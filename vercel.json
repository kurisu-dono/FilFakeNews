{
    "version": 2,
    "builds": [
        {
            "src": "*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "main.py"
        }
    ],
    "functions": {
      "api/**/*.py": {
        "excludeFiles": "**/*.html"
      }
   }
}